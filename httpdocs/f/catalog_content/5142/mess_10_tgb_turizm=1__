<script type="text/javascript">
BX.ready(function(){VK.init({apiId: '2790893'});});
var redirectUrl = window.location.href.replace(window.location.search,"").replace(/(\/)$/,"");
function BxVKAuthInfo(response) 
{
	
	var url_err = redirectUrl + '/?auth_service_id=VKontakte&auth_service_error=1';	
	if(response.session) 
	{
		var url_post = '/bitrixajax/?login=yes';
		var url_ok = redirectUrl;
		var data = {
			"auth_service_id": "VKontakte",
			"vk_session": response.session
		};
		BX.ajax.post(url_post, data, function(res){ window.location = (res == "OK"? url_ok : url_err);});
	} 
	else 
	{
		window.location = url_err;
	}
}
</script>

