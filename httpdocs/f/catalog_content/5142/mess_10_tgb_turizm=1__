$(document).ready(function() {
    var params = {
        dataType : 'json',
        data     : {
            AJAX : true
        },
        success  : function(data) {
            for (var id in data.result) {
                $('#' + id + ' a').attr('onclick', 'return false');
                $('#voting_form_20_section').addClass('voting_done');
                $('#' + id).prepend('<span class="vp_digit">' + data.result[id] + '%</span>').animate({width : data.result[id] + "%"}, 700);
            }
        }
    };
    $('#voting_form_20').ajaxForm(params);
});
